{
  "dns": {
    "rules": [
      {
        "domain_suffix": [
          "arpa"
        ],
        "action": "route",
        "server": "dns-dhcp"
      },
      {
        "domain_suffix": [
          "ts.net"
        ],
        "action": "route",
        "server": "dns-tailscale"
      },
      {
        "clash_mode": "direct",
        "server": "dns-local"
      },
      {
        "clash_mode": "global",
        "server": "dns-fakeip"
      },
      {
        "inbound": [
          "socks-in",
          "mixed-in",
          "tun-in"
        ],
        "action": "route",
        "server": "dns-fakeip"
      },
      {
        "rule_set": [
          "geosite-cn",
          "direct_chao",
          "apple",
          "china_max",
          "download",
          "lan"
        ],
        "action": "route",
        "server": "dns-local"
      },
      {
        "rule_set": [
          "claude",
          "copilot",
          "gemini",
          "openai",
          "game_chao",
          "game",
          "disney",
          "netflix",
          "hbo_usa",
          "spotify",
          "tiktok",
          "youtube",
          "youtube_music",
          "proxy_chao",
          "developer",
          "amazon_cloud",
          "google",
          "github"
        ],
        "action": "route",
        "server": "dns-fakeip"
      }
    ]
  }
}
