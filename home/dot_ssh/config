Include ~/.orbstack/ssh/config

Host *
    ServerAliveInterval 60
    ServerAliveCountMax 3
    TCPKeepAlive yes

Host Fedora
  HostName 100.100.2.3
  User chao
  AddkeysToAgent yes
  IdentityFile ~/.ssh/keys/Fedora/Fedora
  # RemoteCommand /bin/bash

Host Fedora.local
  HostName 192.168.2.3
  User chao
  AddkeysToAgent yes
  IdentityFile ~/.ssh/keys/Fedora/Fedora

Host HA
  HostName homeassistant.local
  User root
  AddkeysToAgent yes
  IdentityFile ~/.ssh/keys/HomeAssistant/HomeAssistant

Host aws-ec2
  HostName er-meng.com
  User chao
  AddkeysToAgent yes
  IdentityFile ~/.ssh/keys/AWS_KP/AWS_KP
